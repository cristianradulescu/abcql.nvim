{
  "runtime": {
    "version": "LuaJIT",
    "path": [
      "lua/?.lua",
      "lua/?/init.lua"
    ]
  },
  "diagnostics": {
    "globals": [
      "vim"
    ],
    "disable": [],
    "groupSeverity": {
      "strong": "Warning",
      "strict": "Warning"
    },
    "groupFileStatus": {
      "strict": "Opened"
    },
    "unusedLocalExclude": [
      "_*"
    ]
  },
  "workspace": {
    "library": [
      "${3rd}/luv/library",
      "${3rd}/busted/library"
    ],
    "checkThirdParty": false
  },
  "completion": {
    "callSnippet": "Replace"
  },
  "hint": {
    "enable": true,
    "setType": true,
    "paramName": "All",
    "paramType": true,
    "arrayIndex": "Disable"
  },
  "format": {
    "enable": false
  },
  "type": {
    "castNumberToInteger": true
  },
  "doc": {
    "privateName": [
      "^_"
    ]
  }
}
